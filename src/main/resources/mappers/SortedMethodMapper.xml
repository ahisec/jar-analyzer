<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.n1ar4.jar.analyzer.core.mapper.SortedMethodMapper">
    <resultMap id="classMap" type="me.n1ar4.jar.analyzer.entity.SortedMethodEntity">
        <result column="sorted_index" property="sortedIndex"/>
        <result column="class_name" property="className"/>
        <result column="method_name" property="methodName"/>
        <result column="method_desc" property="methodDesc"/>
        <result column="jar_id" property="jarId"/>
    </resultMap>
    <insert id="insertMethods">
        INSERT INTO taint_sorted_method
        (sorted_index, class_name, method_name, method_desc, jar_id)
        VALUES
        <foreach collection="list" separator="," item="item">
            (#{item.sortedIndex}, #{item.className}, #{item.methodName}, #{item.methodDesc}, #{item.jarId})
        </foreach>
    </insert>
</mapper>